{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"chrome": "49",
					"firefox": "45",
					"edge": "85",
					"safari": "10",
					"opera": "36"
				},
				"useBuiltIns": "entry",
				"corejs": "3.6.5"
			}
		],
		"@babel/preset-typescript"
	],
	"plugins": [
		["@babel/plugin-transform-modules-umd", { "globals": { "bowser": "bowser" } }]
	]
}
