{
	"name": "vuejs",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite --mode modern --base ''",
		"build": "run-p  \"build-only {@}\" --",
		"preview:modern": "vite preview --mode modern --base ''",
		"preview:legacy": "vite preview --mode legacy --base ''",
		"build-only:modern": "vite build --mode modern",
		"build-only:legacy": "vite build --mode legacy && npx babel dist/legacy/assets --out-dir dist/legacy/assets --ignore \"*.css\"",
		"build-only": "npm run build-only:modern && npm run build-only:legacy && npm run postbuild",
		"postbuild": "cp index-build.html dist/index.html",
		"type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write src/ index.html index-build.html package.json vite.config.ts"
	},
	"dependencies": {
		"@codemirror/lang-go": "^6.0.1",
		"@codemirror/lang-html": "^6.4.9",
		"@codemirror/lang-json": "^6.0.1",
		"@codemirror/lang-markdown": "^6.3.2",
		"@codemirror/theme-one-dark": "^6.1.2",
		"@headlessui/vue": "^1.7.23",
		"@popperjs/core": "^2.11.8",
		"@tiptap/extension-color": "^2.11.7",
		"@tiptap/extension-list-item": "^2.11.7",
		"@tiptap/extension-text-style": "^2.11.7",
		"@tiptap/extension-underline": "^2.11.7",
		"@tiptap/pm": "^2.11.7",
		"@tiptap/starter-kit": "^2.11.7",
		"@tiptap/vue-3": "^2.11.7",
		"apexcharts": "^4.7.0",
		"codemirror": "^6.0.1",
		"pinia": "^3.0.1",
		"qrcode": "^1.5.4",
		"sanitize-html": "^2.15.0",
		"uuid": "^9.0.1",
		"vue": "^3.5.13",
		"vue-codemirror": "^6.1.1",
		"vue-i18n": "^10.0.6",
		"vue-qrcode-reader": "^5.7.2",
		"vue-router": "^4.3.0",
		"vue3-apexcharts": "^1.8.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.28.0",
		"@babel/core": "^7.28.0",
		"@babel/plugin-transform-modules-commonjs": "^7.27.1",
		"@babel/preset-env": "^7.28.0",
		"@rushstack/eslint-patch": "^1.10.1",
		"@tailwindcss/typography": "^0.5.16",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/node": "^20.12.3",
		"@types/qrcode": "^1.5.5",
		"@types/sanitize-html": "^2.15.0",
		"@types/uuid": "^9.0.8",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^13.0.0",
		"@vue/test-utils": "^2.4.5",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-vue": "^9.24.0",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"rollup-plugin-visualizer": "^6.0.3",
		"tailwindcss": "^3.4.3",
		"terser": "^5.43.1",
		"typescript": "~5.4.3",
		"vite": "^6.2.5",
		"vite-plugin-vue-devtools": "^7.7.7",
		"vite-svg-loader": "^5.1.0",
		"vue-tsc": "^2.0.7"
	}
}
